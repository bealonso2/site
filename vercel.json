{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "stories.balonso.com"
        }
      ],
      "destination": "https://brian.bearblog.dev/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "blog.balonso.com"
        }
      ],
      "destination": "https://balonso.com/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.balonso.com"
        }
      ],
      "destination": "https://balonso.com/$1",
      "permanent": true
    }
  ]
}